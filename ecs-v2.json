{
  "containerDefinitions": [
   {
     "name": "wp-widiani",
     "image": "wordpress:latest",
     "cpu": 0,
     "memory": 128,
     "portMappings": [
       {
         "containerPort": 80
       }
     ],
     "dockerLabels": {
       "container" : "wp-widiani",
       "environtment" : "development"
      },
      "logConfiguration" : {
        "logDriver": "json-file",
        "options": {
          "labels" : "container, environment"
        }
     }
   }]
}
